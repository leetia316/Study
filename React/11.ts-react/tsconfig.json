{
  "compilerOptions": {
   "outDir": "./dist" ,
   "sourceMap": true,
   "strict": true,
   "noImplicitAny": true,
   "strictNullChecks": true,
   "module": "commonjs",
   "target": "es5",
   "jsx": "react",
   "esModuleInterop": true,
   "baseUrl": ".",
   "paths": {
     "@/*": [
       "src/*"
     ]
   }
  },
  "include": [
    "./src/**/*"
  ]
}